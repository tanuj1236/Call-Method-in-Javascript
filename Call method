<body>
    <script>
      const person = {
          FullName:function()
          {
            return  this.firstname + this.lastname;
          }
      } 
      const person1={
          firstname:"tanuj",
          lastname:"kumar"
      }

      const person2={
          firstname:"vipul",
          lastname:"pal"
      }

     console.log(person.FullName.call(person1));
    console.log(person.FullName.call(person2));
      
      

    </script>
</body>
